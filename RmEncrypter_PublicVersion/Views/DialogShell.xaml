<Window x:Class="RmEncrypter_PublicVersion.Views.DialogShell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RmEncrypter_PublicVersion"
        xmlns:viewmodels="clr-namespace:RmEncrypter_PublicVersion.ViewModels" 
        xmlns:prism="http://prismlibrary.com/"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance Type=viewmodels:DialogShellViewModel}"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="RmEncrypter"
        Height="455" Width="500"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        Style="{StaticResource WindowStyle}"
        Padding="70,10,70,10">
    <DockPanel>
        <ContentControl Focusable="False" prism:RegionManager.RegionName="{x:Static local:AppRegions.DialogShellContentRegion}"/>
    </DockPanel>
</Window>
